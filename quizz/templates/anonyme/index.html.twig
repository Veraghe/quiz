{% extends 'base.html.twig' %}

{% block title %}Anonyme index{% endblock %}

{% block body %}
    <h1>Anonyme index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for anonyme in anonymes %}
            <tr>
                <td>{{ anonyme.id }}</td>
                <td>{{ anonyme.Email }}</td>
                <td>
                    <a href="{{ path('anonyme_show', {'id': anonyme.id}) }}">show</a>
                    <a href="{{ path('anonyme_edit', {'id': anonyme.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('anonyme_new') }}">Create new</a>
{% endblock %}
